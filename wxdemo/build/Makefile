PROJECTS := wxchartviewer helloworld wxdemo realtimesweep realtimemultithread realtimemeasure realtimeviewport

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

wxchartviewer:
ifneq (,$(wxchartviewer_config))
	@echo "==== Building wxchartviewer ($(wxchartviewer_config)) ===="
	@${MAKE} --no-print-directory -C . -f wxchartviewer.make config=$(wxchartviewer_config)
endif

